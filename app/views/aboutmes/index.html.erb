<p id="notice"><%= notice %></p>

<h1>About me</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Bio</th>
      <th>Create Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @aboutmes.each do |aboutme| %>
      <tr>
        <td><%= aboutme.Name %></td>
        <td><%= aboutme.bio %></td>
        <td><%= aboutme.creationdateandtime %></td>
        <td><%= link_to 'Show', aboutme %></td>
        <td><%= link_to 'Edit', edit_aboutme_path(aboutme) %></td>
        <td><%= link_to 'Destroy', aboutme, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Aboutme', new_aboutme_path %>
